@model List<PCS.Models.Registration>
@{
    ViewBag.Title = "All";
    Layout = "~/Views/Shared/_Back.cshtml";
}
<h3 class="text-center">All Registered Documents</h3>
    <div class="container">
        <div class="white-bg">
            <div class="row">
                <div class="col-lg-12 form-part">
                
                    <table id="My" class="table table-striped table-bordered" style="width:100%">
                        <thead>
                            <tr>
                                <th>Sr #</th>
                                <th>Name</th>
                                <th>Cnic</th>
                                <th>Hospital Name</th>
                                <th>Preferd Email</th>
                                <th>Hospital Telephone</th>
                                <th>Number</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var lst in Model)
                            {
                                <tr>
                                    <td>@lst.id</td>
                                    <td>@lst.Name</td>
                                    <td>@lst.Cnic</td>
                                    <td>@lst.HosName</td>
                                    <td>@lst.PreEmail</td>
                                    <td>@lst.HospTel</td>
                                    <td>@lst.Mobile</td>
                                    <td>
                                        <a href="@Url.Action("Report",new { id = lst.id})" class="btn btn-primary" target="_blank"><i class="fa fa-print"></i></a>
                                    </td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
